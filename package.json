{
    "name": "red-salud-monorepo",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@9.1.0",
    "scripts": {
        "dev": "pnpm --filter red-salud-web dev",
        "build": "pnpm --filter @red-salud/types build && pnpm --filter red-salud-web build",
        "start": "pnpm --filter red-salud-web start",
        "web:dev": "pnpm --filter red-salud-web dev",
        "tauri:dev": "pnpm --filter @red-salud/farmacia-desktop tauri:dev",
        "tauri:farmacia:dev": "pnpm --filter @red-salud/farmacia-desktop tauri:dev",
        "tauri:medico:dev": "pnpm --filter red-salud-desktop-medico tauri dev",
        "lint": "cd apps/web && npx eslint ."
    },
    "dependencies": {
        "html2canvas": "^1.4.1",
        "jspdf": "^4.1.0",
        "qrcode": "^1.5.4"
    },
    "devDependencies": {
        "@types/qrcode": "^1.5.6",
        "next": "^16.1.0"
    }
}